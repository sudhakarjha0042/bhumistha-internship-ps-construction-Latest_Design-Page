{
    "version": 2,
    "builds": [
      {
        "src": "ps/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"  // Corrected the distDir value
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/staticfiles_build/$1"  // Corrected the destination path
      },
      {
        "src": "/(.*)",
        "dest": "ps/wsgi.py"
      }
    ]
  }
  